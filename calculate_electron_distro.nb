Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"If", " ", "using", " ", "Maxwell"}], "-", 
    RowBox[{
    "Boltzmann", " ", "distribution", " ", "then", " ", "use", " ", "this", 
     " ", "cell", " ", "to", " ", "find", " ", "energy", " ", "values", " ", 
     "for", " ", "each", " ", "electron"}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "This", " ", "cell", " ", "will", " ", "run", " ", "the", " ", 
    "calculation", " ", "in", " ", "parallel", " ", "so", " ", "make", " ", 
    "sure", " ", "you", " ", "are", " ", "set", " ", "up", " ", "to", " ", 
    "do", " ", "this", " ", "before", " ", "execution", " ", "of", " ", 
    "calculation"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "Since", " ", "this", " ", "cell", " ", "uses", " ", "a", " ", "fully", 
    " ", "analytical", " ", "integration", " ", "we", " ", "will", " ", "be", 
    " ", "able", " ", "to", " ", "get", " ", "the", " ", "energy", " ", 
    "value", " ", "for", " ", "all", " ", "n", " ", 
    RowBox[{"electrons", ".", " ", "Each"}], " ", "value", " ", "is", " ", 
    "the", " ", "average", " ", "of", " ", "the", " ", "upper", " ", "and", 
    " ", "lower", " ", "limit", " ", "as", " ", "selected", " ", "in", " ", 
    "the", " ", "same", " ", "method", " ", "as", " ", "the", " ", "previous",
     " ", "cell"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"solutions", "=", 
     RowBox[{"a", "/.", 
      RowBox[{"Parallelize", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Quiet", "[", 
          RowBox[{"Solve", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Integrate", "[", 
              RowBox[{
               RowBox[{"energy", "[", "x", "]"}], ",", 
               RowBox[{"{", 
                RowBox[{"x", ",", "0", ",", "a"}], "}"}]}], "]"}], "\[Equal]",
              "i"}], ",", "a"}], "]"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "0", ",", "n"}], "}"}]}], "]"}], "]"}]}]}], ";"}],
    "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"diffs", "=", 
     RowBox[{"Parallelize", "[", 
      RowBox[{"Table", "[", 
       RowBox[{
        FractionBox[
         RowBox[{
          RowBox[{"solutions", "[", 
           RowBox[{"[", 
            RowBox[{"i", "+", "1"}], "]"}], "]"}], "+", 
          RowBox[{"solutions", "[", 
           RowBox[{"[", "i", "]"}], "]"}]}], "2"], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "n"}], "}"}]}], "]"}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"DiscretePlot", "[", 
    RowBox[{
     RowBox[{"energy", "[", "x", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"Flatten", "[", "diffs", "]"}]}], "}"}], ",", 
     RowBox[{"Joined", "\[Rule]", "False"}], ",", 
     RowBox[{"Filling", "\[Rule]", "None"}], ",", 
     RowBox[{"ImageSize", "\[Rule]", "Full"}]}], "]"}], "\[IndentingNewLine]", 
   RowBox[{"Column", "[", 
    RowBox[{"Flatten", "[", "diffs", "]"}], "]"}]}]}]], "Input",
